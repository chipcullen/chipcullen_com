{% extends "./layout.njk" %}

{% block content %}
  <h1>{{ title }}</h1>
  {# {{ collections.post | console | safe }} #}

  {{ content | safe }}

  <ul>
  {% for post in collections.posts | reverse %}
    <li>
      <a href="{{post.url}}">{{ post.data.title }}</a>
      <p>{{ post.data.date | postDate }}</p>
    </li>
  {% endfor %}
  </ul>


{% endblock %}
